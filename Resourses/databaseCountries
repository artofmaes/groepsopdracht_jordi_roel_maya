create table if not exists php2_steden.images
(
	img_id mediumint auto_increment
		primary key,
	img_what varchar(512) null,
	img_filename varchar(512) null,
	img_title varchar(512) null,
	img_width mediumint null,
	img_height mediumint null,
	img_country varchar(512) null,
	img_capital varchar(512) null,
	img_population int(15) null,
	img_currency varchar(512) null
)
charset=utf8;

